{
  "extends": "standard",
  "parser": "babel-eslint",
  "plugins": [
    "standard",
    "react"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "quotes": [2, "single"],
    "strict": [2, "never"],
    "no-duplicate-imports": 0,
    "no-unsafe-finally": 0,
    "no-useless-computed-key": 0,
    "no-useless-escape": 0,
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/react-in-jsx-scope": 2
  }
}
